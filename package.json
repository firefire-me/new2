{
  "keywords": [
    "react",
    "reactjs",
    "argon",
    "argon-react",
    "design",
    "design-react",
    "argon-design",
    "argon-design-react",
    "kit",
    "react-kit",
    "argon-design-system",
    "argon-design-system-react",
    "design-system-react"
  ],
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@umijs/plugin-sass": "^1.1.1",
    "bootstrap": "4.3.1",
    "classnames": "2.3.1",
    "headroom.js": "0.12.0",
    "moment": "2.29.4",
    "nouislider": "14.1.1",
    "react": "17.x",
    "react-datetime": "3.1.1",
    "react-dom": "17.x",
    "react-router-dom": "5.1.2",
    "react-scripts": "5.0.1",
    "reactstrap": "8.4.1",
    "sass": "1.53.0",
    "umi": "^3.5.34"
  },
  "devDependencies": {
    "@types/googlemaps": "3.39.2",
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.34",
    "eslint-plugin-flowtype": "8.0.3",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9",
    "jquery": "3.6.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "umi-plugin-icons": "^0.1.1",
    "yorkie": "^2.0.0"
  }
}
